#!/usr/bin/python
from proc_yields import ProcYields
from disp_yields import DispYields
import sys

"""
py [dtyp] [simnum=siml] [dbg=False]
"""
dtyp=sys.argv[1]

if len(sys.argv)>2:#i.e. simnum entered by user
	simnum=sys.argv[2]
else:
	simnum='siml'

if len(sys.argv)>3:#i.e. dbg entered by user
        dbg=sys.argv[3]
else:
        dbg=False

py=ProcYields(dtyp,simnum=simnum,dbg=dbg)
py.execute()
